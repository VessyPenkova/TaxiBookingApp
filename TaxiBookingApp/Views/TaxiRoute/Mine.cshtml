@model IEnumerable<TaxiRouteServiceModel>
@{
    ViewBag.title = "My TaxiRoutes";
}
<h2 class="text-center">@ViewBag.Title</h2>
<hr />


@if (Model.Any())
{
    <div class="row">
        @foreach (var taxiRoute in Model)
        {
            <partial name="_TaxiRoutePartial" model="@taxiRoute" />
        }
    </div>
}
else
{
    <h3 class="text-center">You have no houses yet!</h3>
}